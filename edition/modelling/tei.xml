          <note type="settled">Settled</note>
          <persName ref="#drake">Derius Drake</persName>
          <desc>To ax dlvd By Puffer 9/1</desc>
          <measure type="currency" unit="dollar" quantity="1.50">1 50</measure>
        </div>
        
